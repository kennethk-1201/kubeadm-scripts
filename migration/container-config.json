{
  "metadata": {
    "name": "nginx-container"
  },
  "image": {
    "image": "docker.io/library/nginx:latest"
  },
  "command": [
    "nginx",
    "-g",
    "daemon off;"
  ],
  "log_path": "nginx-container.log",
  "linux": {},
  "annotations": {},
  "envs": [
    {
      "key": "NGINX_PORT",
      "value": "80"
    }
  ]
}
